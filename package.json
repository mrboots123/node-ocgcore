{
  "name": "node-ocgcore",
  "version": "1.0.2",
  "description": "Node.js FFI bindings for OCGCore (Yu-Gi-Oh! card game engine)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "test": "tsx src/index.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "ocgcore",
    "yugioh",
    "card-game",
    "ffi",
    "native-bindings"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrboots123/node-ocgcore.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.8.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "koffi": "^2.9.2"
  },
  "peerDependencies": {},
  "optionalDependencies": {}
}
